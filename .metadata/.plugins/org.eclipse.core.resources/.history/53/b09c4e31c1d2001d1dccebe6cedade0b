package ch2;

public class Do {

	public static void main(String[] args) {
		String name = "Ja";
		int num = 25;
		int non = 45;		
		double kr = 300;
		int re = 300;
		System.out.println(name);
        System.out.println(num);
        System.out.println(non);
        System.out.println(name+num);
        System.out.println(name+num+non);
        System.out.println(name+(num+non));
        System.out.println(name+(num+kr));
        System.out.println(name+(num+re)+re);
        System.out.println("num"+7+7);
        System.out.println(7+7+"num");
        System.out.println("num"+(7+7));
        System.out.println((7+7)+"eee");
        System.out.println("%o,%v,%e,d,s,v);
	}

}
