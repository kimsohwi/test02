package ch2;

public class Do {

	public static void main(String[] args) {
		String name = "Ja";
		int num = 25;
		int non = 45;		
		double kr = 300.1;
		int re = 300;
		System.out.println(name);
        System.out.println(num);
        System.out.println(non);
        System.out.println(name+num);
        System.out.println(name+num+non);
        System.out.println(name+(num+non));
        System.out.println(name+(num+kr));
        System.out.println(name+(num+kr)+re);
	}

}
